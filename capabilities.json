{
    "dataRoles": [
        {
            "displayName": "Dataset",
            "name": "dataset",
            "kind": "GroupingOrMeasure",
            "description": "Columns to should be part of the table."
        },
        {
            "displayName": "Identity (optional)",
            "name": "identity",
            "kind": "GroupingOrMeasure",
            "description": "Optional identifier which will be hidden in the table, but can be used for drill down purposes"
        },
        {
            "displayName": "Sort keys (optional)",
            "name": "sort",
            "kind": "GroupingOrMeasure",
            "description": "Columns matching the name in Dataset will use value given here as sort key. Example field with display name in dataset 'Price' can be formatted with currency sign and additional properties, such that it is interpreted by Power Bi as an string. The actual value however is a number, to ensure that sorting works based on that underlying value if the value backing field is part of this set and their display names are exactly matching this field will be used for sorting."
        }
    ],
    "drilldown": {
        "roles": [ "identity" ]
    },
    "dataViewMappings": [
        {
            "table": {
                "rows": {
                    "select": [
                        { "for": { "in": "dataset" } },
                        { "bind": { "to": "identity" } },
                        { "bind": { "to": "sort" } }
                    ],
                    "dataReductionAlgorithm": {
                        "top": { "count": 30000 }
                    }
                }
            }
        }
    ],
    "objects": {
        "general": {
            "displayName": "General",
            "properties": {
                "formatString": {
                    "type": { "formatting": { "labelDisplayUnits": false } }
                },
                "js": {
                    "type": {
                        "text": true
                    }
                },
                "css": {
                    "type" : {
                        "text": true
                    }
                }
            }
        },
        "margin" : {
            "displayName": "Margin",
            "properties": {
                "top": {
                    "displayName": "Top",
                    "type": {
                        "numeric": true
                    }
                },
                "bottom": {
                    "displayName": "Bottom",
                    "type": {
                        "numeric": true
                    }
                },
                "left": {
                    "displayName": "Left",
                    "type": {
                        "numeric": true
                    }
                },
                "right": {
                    "displayName": "Right",
                    "type": {
                        "numeric": true
                    }
                }
            }
        },
        "colors" : {
            "displayName": "Colors",
            "properties": {
                "color1": {
                    "displayName": "Color 1",
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "color2": {
                    "displayName": "Color 2",
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "color3": {
                    "displayName": "Color 3",
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "color4": {
                    "displayName": "Color 4",
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "color5": {
                    "displayName": "Color 5",
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "color6": {
                    "displayName": "Color 6",
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "color7": {
                    "displayName": "Color 7",
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "color8": {
                    "displayName": "Color 8",
                    "type": { "fill": { "solid": { "color": true } } }
                }
            }
        },
        "table": {
            "displayName": "Table",
            "properties": {
                "sorting": {
                    "type": {
                        "bool": true
                    }
                },
                "dataSelectable": {
                    "type": {
                        "bool": true
                    }
                },
                "header": {
                    "type": {
                        "bool": true
                    }
                },
                "fallbackImage": {
                    "type": {
                        "text": true
                    }
                },
                "emptyStateHtml": {
                    "type": {
                        "text": true
                    }
                }
            }
        },
        "pagination": {
            "displayName": "Pagination",
            "properties": {
                "show": { "type": { "bool": true }},
                "pageSize": {
                    "type": {
                        "integer": true
                    }
                },
                "paginationItemCount": {
                    "type": {
                        "integer": true
                    }
                }
            }
        }
    },
    "sorting": {
        "custom": {} 
    },
    "advancedEditModeSupport": 2,
    "suppressDefaultTitle": true,
    "privileges": [
        {
            "name": "ExportContent",
            "essential": false
        },
        {
            "name": "WebAccess",
            "essential": true,
            "parameters": [ "*" ]
        }
    ]
}
