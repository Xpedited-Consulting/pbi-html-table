{
    "dataRoles": [
        {
            "displayName": "Dataset",
            "name": "dataset",
            "kind": "GroupingOrMeasure",
            "description": "Columns to should be part of the table."
        },
        {
            "displayName": "Identity (optional)",
            "name": "identity",
            "kind": "GroupingOrMeasure",
            "description": "Optional identifier which will be hidden in the table, but can be used for drill down purposes"
        },
        {
            "displayName": "Sort keys (optional)",
            "name": "sort",
            "kind": "GroupingOrMeasure",
            "description": "Columns matching the name in Dataset will use value given here as sort key. Example field with display name in dataset 'Price' can be formatted with currency sign and additional properties, such that it is interpreted by Power Bi as an string. The actual value however is a number, to ensure that sorting works based on that underlying value if the value backing field is part of this set and their display names are exactly matching this field will be used for sorting."
        }
    ],
    "drilldown": {
        "roles": [ "identity" ]
    },
    "dataViewMappings": [
        {
            "table": {
                "rows": {
                    "select": [
                        { "for": { "in": "dataset" } },
                        { "bind": { "to": "identity" } },
                        { "bind": { "to": "sort" } }
                    ],
                    "dataReductionAlgorithm": {
                        "top": { "count": 30000 }
                    }
                }
            }
        }
    ],
    "objects": {
        "general": {
            "displayName": "Styling",
            "properties": {
                "customCssEnabled": {
                    "type": {
                        "bool": true
                    }
                },
                "css": {
                    "displayName": "Custom CSS",
                    "type" : {
                        "text": true
                    }
                },
                "_cssTextInput": {
                    "type" : {
                        "text": true
                    }
                },
                "cssStrategy": {
                    "type": {
                        "enumeration":[
                            { "value": "replace", "displayName": "Replaces default CSS" },
                            { "value": "overwrite", "displayName": "Overwrites default CSS" }
                        ]
                    }
                }
            }
        },
        "margin" : {
            "displayName": "Margin",
            "properties": {
                "top": {
                    "displayName": "Top",
                    "type": {
                        "numeric": true
                    }
                },
                "bottom": {
                    "displayName": "Bottom",
                    "type": {
                        "numeric": true
                    }
                },
                "left": {
                    "displayName": "Left",
                    "type": {
                        "numeric": true
                    }
                },
                "right": {
                    "displayName": "Right",
                    "type": {
                        "numeric": true
                    }
                }
            }
        },
        "table": {
            "displayName": "Table",
            "properties": {
                "sorting": {
                    "type": {
                        "bool": true
                    }
                },
                "overflow": {
                    "type": {
                        "enumeration": [
                            { "value": "", "displayName": "Hidden" },
                            { "value": "scrollX", "displayName": "Scroll horizontal" },
                            { "value": "scrollY", "displayName": "Scroll vertical" },
                            { "value": "scroll", "displayName": "Scroll" }
                        ]
                    }
                },
                "dataSelectable": {
                    "type": {
                        "bool": true
                    }
                },
                "convertAnchors": {
                    "type": {
                        "bool": true
                    }
                },
                "header": {
                    "type": {
                        "enumeration":[
                            { "value": "normal", "displayName": "Normal" },
                            { "value": "sticky", "displayName": "Sticky" },
                            { "value": "hidden", "displayName": "Hidden" }
                        ]
                    }
                },
                "fallbackImage": {
                    "displayName": "Fallback image URL",
                    "type": {
                        "text": true
                    },
                    "rule": {
                        "inputRole": "dataset",
                        "output": {
                          "property": "fallbackImage"
                        }
                    }
                },
                "emptyStateHtml": {
                    "displayName": "Empty state HTML",
                    "type": {
                        "text": true
                    }
                }
            }
        },
        "pagination": {
            "displayName": "Pagination",
            "properties": {
                "show": { "type": { "bool": true }},
                "pageSize": {
                    "displayName": "Page size",
                    "type": {
                        "integer": true
                    }
                },
                "paginationItemCount": {
                    "displayName": "Number of pagination items",
                    "type": {
                        "integer": true
                    }
                }
            }
        }
    },
    "sorting": {
        "default": {},
        "custom": {} 
    },
    "advancedEditModeSupport": 2,
    "suppressDefaultTitle": true,
    "privileges": [
        {
            "name": "ExportContent",
            "essential": false
        },
        {
            "name": "WebAccess",
            "essential": true,
            "parameters": [ "*" ]
        }
    ]
}
